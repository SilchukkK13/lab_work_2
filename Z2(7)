#include <iostream>
using namespace std;
int main() 
{
   unsigned int time=9000;
   cout<<"Enter time:";
   cin >>time;
    int hours=time/3600;
    int min=time/60-hours*60;
	cout<<"hours"<<hours<<"min"<<min<<endl;
	system("pause");
	}
