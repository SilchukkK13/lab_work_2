#include <iostream>
using namespace std;
int main(int argc, char** argv) 
{
int a,b;
float al,bl;
a=5;
b=3;
cout<<"a%b="<<a%b<<endl;
cin>>a>>b;
al=5;
bl=3;
cout<<"al/bl="<<al/bl<<endl;
cin>>al>>bl;
system ("pause");
return 0;
}
